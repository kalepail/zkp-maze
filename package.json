{
	"name": "noir-maze-challenge",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "CLOUDFLARE_ENV=dev vite",
		"build": "tsc -b && CLOUDFLARE_ENV='' vite build",
		"preview": "pnpm run build && CLOUDFLARE_ENV='' vite preview",
		"deploy": "pnpm run build && wrangler deploy --env=''",
		"types": "wrangler types",
		"lint": "eslint .",
		"typecheck": "tsc -b --noEmit",
		"generate": "python3 generate_maze.py 2918957128 --no-preview",
		"compile": "cd circuit && nargo compile && cp target/circuit.json ../worker/container_src/circuit.json",
		"execute": "cd circuit && nargo execute && cp target/circuit.gz ../worker/container_src/circuit.gz",
		"nargo": "pnpm run compile && pnpm run execute && cd circuit && nargo test"
	},
	"devDependencies": {
		"@aztec/bb.js": "^0.85.0",
		"@cloudflare/containers": "^0.0.28",
		"@cloudflare/vite-plugin": "^1.13.12",
		"@eslint/js": "^9.37.0",
		"@noir-lang/noir_js": "^1.0.0-beta.9",
		"@tailwindcss/vite": "^4.1.14",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@vitejs/plugin-react-swc": "^4.1.0",
		"eslint": "^9.37.0",
		"eslint-plugin-react-hooks": "^7.0.0",
		"eslint-plugin-react-refresh": "^0.4.23",
		"globals": "^16.4.0",
		"hono": "^4.9.12",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tailwindcss": "^4.1.14",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.0",
		"vite": "^7.1.9",
		"wrangler": "^4.42.2"
	}
}